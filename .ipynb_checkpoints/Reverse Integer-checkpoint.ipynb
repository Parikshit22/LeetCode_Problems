   "source": [
    "x =-123\n",
    "\n",
    "arr = []\n",
    "is_neg = x<0\n",
    "\n",
    "x = abs(x)\n",
    "while x>0:\n",
    "    arr.append(x%10)\n",
    "    x = x//10\n",
    "\n",
    "n = len(arr)-1\n",
    "result = 0\n",
    "for i in arr:\n",
    "    result+= i*(10**n)\n",
    "    n-=1\n",
    "if is_neg:\n",
    "    result *= -1\n",
    "result"
   ]
